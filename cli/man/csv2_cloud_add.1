.so ../man/_header.so
\fBcloudscheduler cloud add\fR \- add cloud definitions to cloudscheduler version 2 (csv2) servers
.\" --------------------------------------------------------------------
.SH SYNOPSIS
.\" --------------------------------------------------------------------
.SY cloudscheduler
.B cloud
.B add
.BI "\-ca " "address"
.BI "\-cn " "name"
.BI "\-cpw " "password"
.BI "\-cp " "project"
.BI "\-cr " "region"
.BR "\-ct " "[" "amazon" " | " "azure" " | " "google" " | " "local" " | " "opennebula" " | " "openstack" "]"
.BI "\-cu " "user"
.RI "[" "options" " ...]"
.YS
.\" --------------------------------------------------------------------
